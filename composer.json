{
    "name": "toolsets/builder",
    "description": "Application builder for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["Builder", "laravel"],
    "authors": [
        {
            "name": "Raf",
            "email": "raftalks@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Toolsets\\Builder\\": "lib/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/BuilderTestCase.php"
        ]
    },
    "require": {
        "php": ">=5.6.4",
        "symfony/yaml": "^3.2",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "Illuminate/Console": "self.version",
        "illuminate/support": "self.version",
        "illuminate/routing": "self.version",
        "illuminate/container": "self.version",
        "Illuminate/Database": "self.version",
        "Illuminate/Filesystem": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
